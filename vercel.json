{
  "buildCommand": "npm install --prefer-offline --no-audit --legacy-peer-deps && npm run build",
  "installCommand": "npm install --prefer-offline --no-audit --legacy-peer-deps",
  "framework": "nextjs",
  "functions": {
    "app/**/*.{js,ts,tsx}": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "env": {
    "NPM_CONFIG_REGISTRY": "https://registry.npmjs.org/",
    "NPM_CONFIG_TIMEOUT": "60000",
    "NPM_CONFIG_FETCH_TIMEOUT": "60000"
  }
}
